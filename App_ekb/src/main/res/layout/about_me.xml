<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fresco="http://schemas.android.com/apk/res-auto"
    style="@style/immersion_status"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/base_title_color"
    android:orientation="vertical">

    <include layout="@layout/app_base_title" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="143dp"
        android:src="@mipmap/find_banner" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#f2f2f2"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/zoujin_jusheng_about"
            android:layout_width="match_parent"
            android:layout_height="128px"
            android:layout_marginTop="36px"
            android:background="@drawable/checked_linearcolor"
            android:clickable="true"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="56px"
                android:layout_marginRight="56px"
                android:drawablePadding="5dp"
                android:drawableRight="@mipmap/icon_rightto"
                android:text="走进小行家"
                android:textColor="#333333"
                android:textSize="48px" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginLeft="56px"
            android:background="#d2d2d2" />


        <LinearLayout
            android:id="@+id/shintroduce_ll"
            android:layout_width="match_parent"
            android:layout_height="128px"
            android:background="@drawable/checked_linearcolor"
            android:clickable="true"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:visibility="gone">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="56px"
                android:layout_marginRight="56px"
                android:drawablePadding="5dp"
                android:drawableRight="@mipmap/icon_rightto"
                android:text="股东介绍"
                android:textColor="#333333"
                android:textSize="48px" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#d2d2d2"
            android:visibility="gone" />


        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#d2d2d2" />

        <LinearLayout
            android:id="@+id/company_qualification_ll"
            android:layout_width="match_parent"
            android:layout_height="128px"
            android:background="@drawable/checked_linearcolor"
            android:clickable="true"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="56px"
                android:layout_marginRight="56px"
                android:drawablePadding="5dp"
                android:drawableRight="@mipmap/icon_rightto"
                android:text="公司资质"
                android:textColor="#333333"
                android:textSize="48px" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#d2d2d2" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#d2d2d2" />

        <LinearLayout
            android:id="@+id/web_an_ll"
            android:layout_width="match_parent"
            android:layout_height="128px"
            android:background="@drawable/checked_linearcolor"
            android:clickable="true"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="56px"
                android:layout_marginRight="56px"
                android:drawablePadding="5dp"
                android:drawableRight="@mipmap/icon_rightto"
                android:text="网站公告"
                android:textColor="#333333"
                android:textSize="48px" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#d2d2d2" />


    </LinearLayout>


</LinearLayout>